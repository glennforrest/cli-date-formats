{
    "name": "glennforrest/cli-date-formats",
    "description": "CLI app to show commonly used date format tokens in your terminal",
    "keywords": ["cli", "dates", "date formats", "format", "terminal"],
    "homepage": "https://github.com/glennforrest/cli-date-formats",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/glennforrest/cli-date-formats/issues",
        "source": "https://github.com/glennforrest/cli-date-formats"
    },
    "authors": [
        {
            "name": "Glenn Forrest",
            "email": "glennjamesforrest@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/view": "9.0",
        "nunomaduro/termwind": "^1.4"
    },
    "require-dev": {
        "laravel-zero/framework": "9.0",
        "mockery/mockery": "^1.4.3",
        "pestphp/pest": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/date-formats"]
}
